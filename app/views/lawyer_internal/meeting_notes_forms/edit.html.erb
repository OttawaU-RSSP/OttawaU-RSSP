<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @meeting_notes_form, url: lawyer_internal_meeting_notes_form_form_path, method: :put do |form| %>
      <%= form.hidden_field :application_id, value: @application.id %>
      <%= render partial: "form", locals: { meeting_notes_form: @meeting_notes_form, form: form } %>
      <div>
        <%= form.submit 'Submit', class: 'btn btn-primary btn-lg' %>
      </div>
    <% end %>
  </div>
</div>
